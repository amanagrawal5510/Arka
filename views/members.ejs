<%- include("partials/header")-%>
<%- include("partials/navbar")-%>

    <section class="team-boxed" style="background-image: url('assets/img/starskyheader.jpg');">
        <div class="container">
            <div class="intro">
                <h2 class="text-center">Club Members</h2>
            </div>
            <div class="row people" style="box-shadow: inset 0px 0px rgb(0,0,0);">
                <% memberlist.forEach(function(item){%>
                <div class="col-md-6 col-lg-4 item">
                    <div class="box">
                        <% imageslist.forEach(function(image){%> 
                            <% if(image.title == item.name ) { %>
                            <img class="rounded-circle" src="data:<%= image.contentType %>;base64,<%= image.imageBase64 %>" width="100%" height="100%">
                            <% } %>
                        <% })%>
                        
                        <h3 class="name" style="color: rgb(39,41,43);"><%= item.name %></h3>
                        <p class="title"><%= item.year %></p>
                        <p class="description"><%= item.branch %> </p>
                        <div class="social">
                            <a href="<%= item.links.github %>"><i class="fa fa-github"></i></a>
                            <a href="<%= item.links.linkedln %>"><i class="fab fa-linkedin"></i></a>
                            <a href="<%= item.links.email %>"><i class="fas fa-envelope"></i></a>
                            <a href="<%= item.links.phone %>"><i class="fas fa-phone"></i></a>
                            

                            
                        </div>
                    </div>
                </div>
                <% })%>
               
            </div>
        </div>
    </section>
<%- include("partials/footer")-%>    
    